#: ../src/iop/filmicrgb.c:3924 ../src/iop/filmicrgb.c:4624
msgid "display"
msgstr "Näyttö"

#. axis legend
#: ../src/iop/filmicrgb.c:3933
msgid "(%)"
msgstr "(%)"

#. Page SCENE
#: ../src/iop/filmicrgb.c:3942 ../src/iop/filmicrgb.c:4462
msgid "scene"
msgstr "Näkymä"

#. axis legend
